{
  "name": "moni_center2",
  "version": "1.1.1",
  "description": "海洋博物馆",
  "keywords": [
    "electron"
  ],
  "main": "main.js",
  "build": {
    "appId": "ocean.museum.moni_center2",
    "mac": {
    	"icon": "icon.ico",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
    	"icon": "icon.ico",
      "target": [
        "nsis",
        "zip"
      ]
    },
    "files": [
      "package.json",
      "main.js",
      "icon.ico"
    ],
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "perMachine": true
    }
  },
  "scripts": {
    "dev": "electron .",
    "build": "electron-builder"
  },
  "devDependencies": {
    "electron": "^9.1.0",
    "electron-builder": "^20.38.4"
  },
  "dependencies": {
    "electron-updater": "^4.0.6"
  }
}
